<?xml version="1.0" encoding="utf-8"?>
<extension type="System" version="1.0">
    <name>SYSTEM_USERS_NAME</name>
    <author>Amy Stephen</author>
    <create_date>August 2012</create_date>
    <copyright>Copyright (C) 2012 Amy Stephen. All rights reserved.</copyright>
    <license>MIT</license>
    <author_email>hello@niambie.org</author_email>
    <author_url>http://niambie.org</author_url>
    <version>1.0</version>
    <description>SYSTEM_USERS_DESCRIPTION</description>
    <model
            name="Users"
            criteria_catalog_type_id="3000"
            criteria_extension_instance_id="3000"
            check_view_level_access="1"
            extends="UserDatasource">
        <table>
            <joins>
                <join model="Catalog"
                      alias="catalog"
                      select="id,catalog_type_id,source_id,sef_request,view_group_id,redirect_to_id,extension_instance_id"
                      jointo="source_id,application_id,catalog_type_id"
                      joinwith="id,APPLICATION_ID,catalog_type_id"/>
            </joins>
        </table>
    </model>
</extension>
