    <version>1.0</version>
    <description>VIEW_TEMPLATE_FOOTER_DESCRIPTION</description>
    <model name="Adminfooter">
        <customfields>
            <customfield name="parameters">
                <field name="copyright_holder" type="char" null="0" default="Molajo" values=""/>
