        <filter name="translation_of_id" filter="int" null="0" default="0"/>
        <filter name="ordering" filter="int" null="0" default="0"/>
    </filters>
    <fields>
        <field name="custom_fields" registry="CategoryCustomFields">
            <custom_fields name="custom_fields" filter="char" null="0" default="This is the default" values=""/>
        </field>
        <field name="parameters" registry="CategoryParameters">
            <parameters name="display_title" filter="char" null="0" default=""/>
            <parameters name="display_suppress_no_results" filter="binary" null="1" default="1" values="0,1"/>
            <parameters name="catalog_type_id" filter="integer" null="1" default="10000" values="10000" hidden="true"/>
            <parameters name="default_theme_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_page_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_items_template_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_items_wrap_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_item_template_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_item_wrap_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_edit_template_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="default_edit_wrap_view_id" filter="integer" null="1" default="" values=""/>
            <parameters name="caching" filter="binary" null="1" default="1" values="0,1"/>
            <parameters name="cache_time" filter="integer" null="1" default="15" values=""/>
            <parameters name="cache_handler" filter="char" null="1" default="" values=""/>
        </field>
        <field name="metadata" registry="CategoryMetadata">
            <metadata name="metadata_title" filter="char" null="0" default=""/>
            <metadata name="metadata_description" filter="text" null="0" default=""/>
            <metadata name="metadata_keywords" filter="char" null="1" default=""/>
            <metadata name="metadata_robots" filter="char" null="0" default="follow,index"
