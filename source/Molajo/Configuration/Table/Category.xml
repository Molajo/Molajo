        <filter name="parent_id" filter="int" null="0" default="0"/>
        <filter name="lft" filter="int" null="0" default="0"/>
        <filter name="rgt" filter="int" null="0" default="0"/>
        <filter name="home" filter="int" null="0" default="0"/>
        <filter name="custom_fields" filter="" null="1" default=""/>
        <filter name="parameters" filter="" null="1" default=""/>
        <filter name="metadata" filter="" null="1" default=""/>
