            <metadata name="metadata_title" filter="char" null="0" default=""/>
            <metadata name="metadata_description" filter="text" null="0" default=""/>
            <metadata name="metadata_keywords" filter="char" null="1" default=""/>
            <metadata name="metadata_robots" filter="char" null="0" default="follow,index"
