        <filter name="id" filter="" null="1" default="" identity="1"/>
        <filter name="view_group_name_list" filter="char" null="0" default=""/>
        <filter name="view_group_list" filter="int" null="0" default=""/>
    </filters>
    <foreignkeys>
        <foreignkey name="user_id" source_id="id" source_model="Users" required="1"/>
        <foreignkey name="view_group_id" source_id="id" source_model="GroupViewGroup" required="1"/>
    </foreignkeys>
</model>

