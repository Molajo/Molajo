<?xml version="1.0" encoding="utf-8"?>
<model
        name="UserGroups"
        table="#__user_groups"
        primary_key="id"
        data_source="JDatabase">
    <filters>
        <filter name="user_id" filter="int" null="0" default=""/>
        <filter name="group_id" filter="int" null="0" default=""/>
    </filters>
    <foreignkeys>
        <foreignkey name="user_id" source_id="id" source_model="Users" required="1"/>
        <foreignkey name="group_id" source_id="id" source_model="Content" required="1"/>
    </foreignkeys>
</model>

