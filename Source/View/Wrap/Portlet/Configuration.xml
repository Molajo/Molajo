    <author>Amy Stephen</author>
    <create_date>January 2014</create_date>
    <copyright>Copyright (C) 2014 Amy Stephen. All rights reserved.</copyright>
    <license>MIT</license>
    <author_email>hello@molajo.org</author_email>
    <author_url>http://molajo.org</author_url>
    <version>1.0</version>
    <description>VIEW_WRAP_PORTLET_DESCRIPTION</description>
    <model name="Portlet" extends="Wrapview">
        <customfields>
            <customfield name="parameters">
                <field name="display_title" type="boolean" null="0" default="1"/>
                <field name="display_body" type="boolean" null="0" default="1"/>
            </customfield>
        </customfields>
    </model>
</extension>
